{
  "name": "@autospec/dashboard",
  "version": "1.0.0",
  "description": "Real-time monitoring dashboard for AutoSpec projects",
  "type": "module",
  "main": "dist/server/index.js",
  "bin": {
    "autospec-dashboard": "./bin/cli.js"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "cd src/client && vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd src/client && vite build && cp -r dist ../../dist/client",
    "start": "node dist/server/index.js",
    "preview": "cd src/client && vite preview"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "chokidar": "^3.5.3",
    "gray-matter": "^4.0.3",
    "marked": "^11.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "recharts": "^2.10.4",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "lucide-react": "^0.312.0"
  }
}
